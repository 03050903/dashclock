<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Weather -->
    <string name="pref_weather_units_title">Weather units</string>
    <string-array name="pref_weather_units_display_names">
        <item>°F</item>
        <item>°C</item>
    </string-array>
    <string-array name="pref_weather_units_values">
        <item>f</item>
        <item>c</item>
    </string-array>

    <!-- Gmail -->
    <string name="pref_gmail_label_title">Show unread count for label</string>
    <string-array name="pref_gmail_label_display_names">
        <item>Inbox</item>
        <item>Priority Inbox</item>
    </string-array>
    <string-array name="pref_gmail_label_inbox_values">
        <item>i</item>
        <item>p</item>
    </string-array>

    <string name="pref_gmail_accounts_title">Show unread count for accounts</string>
    <plurals name="pref_gmail_accounts_summary_template">
        <item quantity="one">
            <xliff:g id="selected">%1$d</xliff:g> of <xliff:g id="total">%2$d</xliff:g>
            account selected
        </item>
        <item quantity="other">
            <xliff:g id="selected">%1$d</xliff:g> of <xliff:g id="total">%2$d</xliff:g>
            accounts selected
        </item>
    </plurals>
</resources>
